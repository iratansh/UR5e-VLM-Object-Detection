<?xml version="1.0"?>
<sdf version="1.6">
  <world name="ur5e_vision_world">
    
    <!-- Physics with high precision for accurate simulation -->
    <physics name="default_physics" default="0" type="ode">
      <max_step_size>0.001</max_step_size>
      <real_time_factor>1.0</real_time_factor>
      <real_time_update_rate>1000</real_time_update_rate>
      <ode>
        <solver>
          <type>quick</type>
          <iters>50</iters>
          <sor>1.3</sor>
        </solver>
        <constraints>
          <cfm>0.0</cfm>
          <erp>0.2</erp>
          <contact_max_correcting_vel>100.0</contact_max_correcting_vel>
          <contact_surface_layer>0.001</contact_surface_layer>
        </constraints>
      </ode>
    </physics>
    
    <!-- Lighting optimized for computer vision -->
    <light name="sun" type="directional">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>0.8 0.8 0.8 1</diffuse>
      <specular>0.2 0.2 0.2 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>
    
    <!-- Additional lighting for consistent vision -->
    <light name="ambient_light" type="ambient">
      <color>0.4 0.4 0.4 1</color>
    </light>
    
    <!-- Ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>
    
    <!-- Work table -->
    <model name="work_table">
      <pose>0.5 0 0.4 0 0 0</pose>
      <static>true</static>
      <link name="table_link">
        <visual name="table_visual">
          <geometry>
            <box>
              <size>1.2 0.8 0.02</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Wood</name>
            </script>
          </material>
        </visual>
        <collision name="table_collision">
          <geometry>
            <box>
              <size>1.2 0.8 0.02</size>
            </box>
          </geometry>
          <surface>
            <friction>
              <ode>
                <mu>0.8</mu>
                <mu2>0.8</mu2>
              </ode>
            </friction>
          </surface>
        </collision>
        <inertial>
          <mass>50</mass>
          <inertia>
            <ixx>1</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>1</iyy>
            <iyz>0</iyz>
            <izz>1</izz>
          </inertia>
        </inertial>
      </link>
    </model>
    
    <!-- Test objects for VLM detection -->
    
    <!-- Water bottle -->
    <model name="water_bottle">
      <pose>0.45 0.15 0.485 0 0 0</pose>
      <static>false</static>
      <link name="bottle_link">
        <visual name="bottle_visual">
          <geometry>
            <cylinder>
              <radius>0.03</radius>
              <length>0.17</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Blue</name>
            </script>
          </material>
        </visual>
        <collision name="bottle_collision">
          <geometry>
            <cylinder>
              <radius>0.03</radius>
              <length>0.17</length>
            </cylinder>
          </geometry>
        </collision>
        <inertial>
          <mass>0.5</mass>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001</iyy>
            <iyz>0</iyz>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
      </link>
    </model>
    
    <!-- Coffee cup -->
    <model name="coffee_cup">
      <pose>0.35 -0.1 0.46 0 0 0</pose>
      <static>false</static>
      <link name="cup_link">
        <visual name="cup_visual">
          <geometry>
            <cylinder>
              <radius>0.04</radius>
              <length>0.12</length>
            </cylinder>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Red</name>
            </script>
          </material>
        </visual>
        <collision name="cup_collision">
          <geometry>
            <cylinder>
              <radius>0.04</radius>
              <length>0.12</length>
            </cylinder>
          </geometry>
        </collision>
        <inertial>
          <mass>0.3</mass>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001</iyy>
            <iyz>0</iyz>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
      </link>
    </model>
    
    <!-- Book -->
    <model name="book">
      <pose>0.6 0.0 0.425 0 0 0.5</pose>
      <static>false</static>
      <link name="book_link">
        <visual name="book_visual">
          <geometry>
            <box>
              <size>0.15 0.20 0.03</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Green</name>
            </script>
          </material>
        </visual>
        <collision name="book_collision">
          <geometry>
            <box>
              <size>0.15 0.20 0.03</size>
            </box>
          </geometry>
        </collision>
        <inertial>
          <mass>0.2</mass>
          <inertia>
            <ixx>0.001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.001</iyy>
            <iyz>0</iyz>
            <izz>0.001</izz>
          </inertia>
        </inertial>
      </link>
    </model>
    
    <!-- Phone -->
    <model name="phone">
      <pose>0.25 0.2 0.42 0 0 0</pose>
      <static>false</static>
      <link name="phone_link">
        <visual name="phone_visual">
          <geometry>
            <box>
              <size>0.07 0.14 0.01</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Black</name>
            </script>
          </material>
        </visual>
        <collision name="phone_collision">
          <geometry>
            <box>
              <size>0.07 0.14 0.01</size>
            </box>
          </geometry>
        </collision>
        <inertial>
          <mass>0.15</mass>
          <inertia>
            <ixx>0.0001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0001</iyy>
            <iyz>0</iyz>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
      </link>
    </model>
    
    <!-- Small box -->
    <model name="small_box">
      <pose>0.55 -0.15 0.445 0 0 0.3</pose>
      <static>false</static>
      <link name="box_link">
        <visual name="box_visual">
          <geometry>
            <box>
              <size>0.08 0.08 0.09</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Orange</name>
            </script>
          </material>
        </visual>
        <collision name="box_collision">
          <geometry>
            <box>
              <size>0.08 0.08 0.09</size>
            </box>
          </geometry>
        </collision>
        <inertial>
          <mass>0.1</mass>
          <inertia>
            <ixx>0.0001</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.0001</iyy>
            <iyz>0</iyz>
            <izz>0.0001</izz>
          </inertia>
        </inertial>
      </link>
    </model>
    
    <!-- Background objects for visual complexity -->
    <model name="background_wall">
      <pose>1.0 0 0.5 0 0 0</pose>
      <static>true</static>
      <link name="wall_link">
        <visual name="wall_visual">
          <geometry>
            <box>
              <size>0.02 2.0 1.0</size>
            </box>
          </geometry>
          <material>
            <script>
              <name>Gazebo/Grey</name>
            </script>
          </material>
        </visual>
        <collision name="wall_collision">
          <geometry>
            <box>
              <size>0.02 2.0 1.0</size>
            </box>
          </geometry>
        </collision>
      </link>
    </model>
    
    <!-- GUI settings -->
    <gui fullscreen='0'>
      <camera name='user_camera'>
        <pose>1.5 1.5 1.2 0 0.4 -2.4</pose>
        <view_controller>orbit</view_controller>
      </camera>
    </gui>
    
  </world>
</sdf> 